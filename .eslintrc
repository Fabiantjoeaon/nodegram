    {
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "indent": [
            "error", 
            4,
            {
                "MemberExpression": "off"
            }
        ],
        "semi": 0,
        "comma-dangle": 0,
        "keyword-spacing": 0,
        "no-trailing-spaces": 0,
        "object-curly-spacing": 0,
        "no-underscore-dangle": 0,
        "curly": 0,
        "function-paren-newline": ["error", "multiline"],
        "no-multi-assign": 0
    }
}
